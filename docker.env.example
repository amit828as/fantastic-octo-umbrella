# Docker Environment Configuration Example
# Copy this file to .env for docker-compose usage

# Database Configuration
POSTGRES_DB=org_management
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres_docker_password
POSTGRES_PORT=5432

# API Configuration
API_PORT=8000

# JWT Configuration
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production-minimum-32-chars
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# FastAPI Configuration
FASTAPI_ENV=development
DEBUG=true

# Database URL (used by application - automatically set in docker-compose)
# DATABASE_URL=postgresql://postgres:postgres_docker_password@db:5432/org_management 